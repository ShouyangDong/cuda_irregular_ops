extern "C" __mlu_global__ void bmm(float *A, float *B, float *C) {
  __nram__ float A_nram[64 * 256];
  __wram__ float B_wram[256 * 256];
  __nram__ float C_nram[64 * 256];
  if (clusterId < 4) {
    if (coreId < 4) {
      __memcpy(A_nram, A + (clusterId * 4 + coreId) * 64 * 256, 64 * 256 * 4,
               GDRAM2NRAM);
      __memcpy(B_wram, B + (clusterId * 4 + coreId) * 64 * 256, 256 * 256 * 4,
               GDRAM2WRAM);
      __bang_mlp(C_nram, A_nram, B_wram, 64, 256);
      __memcpy(C + (clusterId * 4 + coreId) * 64 * 256, C_nram, 64 * 256 * 4,
               NRAM2GDRAM);
    }
  }
}

